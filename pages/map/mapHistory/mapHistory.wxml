<view class='content'>
  <map id='myMap' longitude='{{longitude}}' latitude='{{latitude}}' markers='{{markerDatas}}' scale='18' circles='{{circles}}' polyline='{{polyline}}'></map>
  <cover-view class="tip" wx:if="{{polyline.length}}">
      <cover-view class="tip_head">
        <cover-view class="tip_head_date">
          {{time}}
        </cover-view>
        <cover-view class="tip_head_time">
          {{timeFnNumber}}
        </cover-view>
      </cover-view>
      <cover-view class="tip_address">
        <cover-view class="tip_address_line"></cover-view>
        <cover-view class="tip_address_top"><cover-view class="circle green"></cover-view><cover-view class="tip_address_text">{{startName}}</cover-view></cover-view>
        <cover-view class="tip_address_top"><cover-view class="circle"></cover-view><cover-view class="tip_address_text">{{endName}}</cover-view></cover-view>
      </cover-view>
