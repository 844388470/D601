/* pages/equipment/manage/manage.wxss */

.content{
  width: 100%;
  height: 100%;
  min-height: 100vh;
  background-color: #f1f1f1;
}
.guize{
  position: absolute;
  bottom: 10rpx;
  left: 20rpx;right: 20rpx;
  font-size: 26rpx;color: #999;text-align: center;
}

.manage{
  padding:35rpx 27rpx;
  border-bottom: 1rpx solid #e4e4e4;
  display:flex;
  align-items:center;
  justify-content:space-between;
  background-color: #fff;
}

.manage .manage_left{
  color: #000;
  font-size: 32rpx;
}

.manage .manage_right{
  color: #a3a3a3;
  font-size: 30rpx;
}

.manage .manage_right_text{
  line-height: 28rpx;
}

.manage .manage_right_image{
  position: relative;
  top: 5rpx;
  width: 16rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.title{
  padding: 27rpx;
  background-color: #f1f1f1;
  color: #000;
  font-size: 32rpx;
}

.manage_list{
  margin:0  27rpx 35rpx;
  padding: 20rpx;
  background-color: #fff;
  display:flex;
  align-items:center;
  justify-content:space-between;
  border-radius: 10rpx;
}

.manage_list .manage_left{
  color: #000;
  font-size: 32rpx;
}
.manage_list .manage_left_image{
  margin-left: 30rpx;
  position: relative;
  top: 5rpx;
  width: 30rpx;
  height: 30rpx;
}
.manage_list .manage_right_image{
  width: 30rpx;
  height: 35rpx;
}