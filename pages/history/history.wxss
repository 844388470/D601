.content>view>view{
  padding:20rpx;
}
.list{
  border:1rpx solid #eee;
  padding: 20rpx;
  font-size:29rpx;
}

.list>view{
  margin-top:10rpx;
}
.list>view:first-child{
  margin-top:0rpx;
}


.list+.list{
  border-top:0;
  box-shadow-top: 0 0 0 0;
}

.fs30+.list{
  margin-top:20rpx;
}

.circle {
  display: inline-block;
  width: 12px;
  height: 12px;
  border-radius: 50%;
  background-color: #CC9999;
  margin-right:20rpx;
}
.green{
  background-color: #99CC99;
}
