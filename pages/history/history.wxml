<view class="content"  wx:if="{{model=='D601003'}}">
    <!-- <picker mode="selector" bindchange="bindPickerChange" range="{{array}}" range-key="name"> -->
        <view class="flexc">
          <view class="">选择设备:</view>
          <view class="flex-flex selector tr">{{indexs}}</view>
          <!-- <view class=''>
            <view class='iconfont tako-icon-youjiantou-copy fs30'></view>
          </view> -->
        </view>
    <!-- </picker> -->
    <view class="flexc">
      <view class="">选择日期:</view>
      <view class="flex-flex selector tr flexr">
        <picker mode="date" bindchange="bindStartDate" class="width100">
          <view class="mr  tr width100">{{startDate}}</view>
        </picker>
      </view>
      <view class=''>
        <view class='iconfont tako-icon-youjiantou-copy fs30'></view>
      </view>
    </view>
    <view class="flexc">
      <view class="">开始时间:</view>
      <view class="flex-flex selector tr flexr">
        <picker mode="time" bindchange="bindStartTime" class="width100">
          <view class="mr  tr width100">{{startTime}}</view>
        </picker>  
      </view>
      <view class=''>
        <view class='iconfont tako-icon-youjiantou-copy fs30'></view>
      </view>
    </view>
    <view class="flexc">
      <view class="">结束时间:</view>
      <view class="flex-flex selector tr flexr">
        <picker mode="time" bindchange="bindEndTime" class="width100">
          <view class="mr  tr width100">{{endTime}}</view>
        </picker>  
      </view>
      <view class=''>
        <view class='iconfont tako-icon-youjiantou-copy fs30'></view>
      </view>
    </view>
    <view class="btn" bindtap='goChaXun'>查询</view>
</view>

<view class="content"  wx:if="{{model!=='D601003'&&model!=='D601'}}">
